{
  "savePost": {
    "params": {
      "newPostParams": "#Post"
    },
    "return": "#Post"
  },
  "getPost": {
    "params": {
      "id": {
        "type": "number"
      }
    },
    "return": "#Post"
  },
  "findPost": {
    "params": {
      "postId": {
        "type": "number",
        "nullable": true
      },
      "authorId": {
        "type": "number",
        "nullable": true
      },
      "page": {
        "type": "Page",
        "nullable": true
      }
    },
    "return": "#Post[]"
  },
  "findPostByAuthor": {
    "params": {
      "authorId": {
        "type": "number"
      }
    },
    "return": "#Post"
  },
  "findAuthorByPost": {
    "params": {
      "postId": "#Post.id",
      "postTitle": "#Post.title"
    },
    "return": "#Author"
  }
}
